@model NHDNUG.Models.ViewModelMeetingEdit

@{
    ViewBag.Title = "Edit Meeting";
}
<script type="text/javascript">
    setAdminNavFocus("meetingsAdmin")
</script>
<h2>@ViewBag.Title</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
Speaker URL:<br /> <a href="http://www.nhdnug.org/meetings/speakeredit/@Model.Meeting.rowguid">http://www.nhdnug.org/meetings/speakeredit/@Model.Meeting.rowguid</a>
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model => model.Meeting.MeetingID)
    @Html.HiddenFor(model => model.Meeting.rowguid)
    @Html.Partial("_CreateOrEdit", Model)
    <p>
        <input type="submit" value="Save" class="k-button" /> | @Html.ActionLink("Back to List", "Index")
    </p>

    
}

@section News {

@Html.Partial("_DocumentModule", Model)

}